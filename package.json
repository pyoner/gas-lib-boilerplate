{
  "name": "gas-lib-template",
  "version": "0.1.0",
  "description": "Google Apps Script lib template",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/lib",
    "prepublish": "npm run compile",
    "postpublish": "rm -fr lib/",
    "test": "BABEL_CACHE_PATH=.cache/babel.json tape -r babel-register tests/local/**/*.js",
    "build": "webpack --config=webpack.config.js",
    "dist": "NODE_ENV=production webpack --config=webpack.config.js",
    "deploy": "gapps upload"
  },
  "keywords": [
    "google",
    "apps",
    "script",
    "gas",
    "template",
    "lib"
  ],
  "author": "Jungle",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-closure-elimination": "^1.0.6",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "bump-webpack-plugin": "^0.1.0",
    "json-loader": "^0.5.4",
    "path-chunk-webpack-plugin": "^1.2.0",
    "remove-webpack-plugin": "^1.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.2"
  }
}
